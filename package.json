{
  "name": "nexus-os",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "predev": "next typegen",
    "dev": "next dev --turbopack",
    "prebuild": "next typegen",
    "build": "next build",
    "start": "next start -H 0.0.0.0 -p 3000",
    "lint": "biome check --fix ./src",
    "format": "biome format --write ./src",
    "prepare": "husky",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "test": "vitest run",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx prisma generate && npx prisma migrate deploy",
    "seed": "npx prisma db seed",
    "inngest:dev": "npx inngest-cli@latest dev -u http://localhost:3000/api/inngest"
  },
  "dependencies": {
    "@getbrevo/brevo": "^3.0.1",
    "@hookform/resolvers": "^5.2.2",
    "@mantine/carousel": "^8.3.11",
    "@mantine/charts": "^8.3.11",
    "@mantine/code-highlight": "^8.3.11",
    "@mantine/core": "^8.3.11",
    "@mantine/dates": "^8.3.11",
    "@mantine/dropzone": "^8.3.11",
    "@mantine/form": "^8.3.11",
    "@mantine/hooks": "^8.3.11",
    "@mantine/modals": "^8.3.11",
    "@mantine/notifications": "^8.3.11",
    "@mantine/nprogress": "^8.3.11",
    "@mantine/spotlight": "^8.3.11",
    "@mantine/tiptap": "^8.3.11",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/adapter-pg": "^7.2.0",
    "@stripe/stripe-js": "^8.6.1",
    "@tabler/icons-react": "^3.36.1",
    "@tanstack/react-query": "^5.90.16",
    "@tiptap/extension-link": "^3.15.3",
    "@tiptap/pm": "^3.15.3",
    "@tiptap/react": "^3.15.3",
    "@tiptap/starter-kit": "^3.15.3",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "inngest": "^3.48.1",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.562.0",
    "mantine-form-zod-resolver": "^1.3.0",
    "next": "16.1.1",
    "next-auth": "^4.24.13",
    "nextjs-routes": "^2.2.5",
    "openai": "^6.15.0",
    "otplib": "^12.0.1",
    "p-limit": "^7.2.0",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "qrcode": "^1.5.4",
    "query-string": "^9.3.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.70.0",
    "recharts": "^3.6.0",
    "stripe": "^20.2.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@prisma/client": "^7.2.0",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "autoprefixer": "^10.4.20",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prisma": "^7.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^1.6.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "biome check --write --diagnostic-level=error"
    ]
  },
  "packageManager": "pnpm@10.27.0",
  "engines": {
    "node": ">=20",
    "pnpm": ">=10"
  },
  "engineStrict": true
  ,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "Safari >= 15.4"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
